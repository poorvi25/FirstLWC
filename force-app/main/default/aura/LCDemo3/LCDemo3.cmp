<aura:component >
    <aura:attribute name="Message1" type="String"/>
    <aura:attribute name="Message2" type="String"/>
    
    
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    Button1 : {!v.Message1}
    <br/><br/>
    Button2 : {!v.Message2}
    <br/><br/>
    
    <lightning:button label="ClickMe" onclick="{!c.handleClick}"/>
    <hr/>
    
    <lightning:button label="ClickMeAsWell" onclick="{!c.handleClick}"/>
    
    
    
</aura:component>